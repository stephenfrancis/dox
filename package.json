{
  "name": "dox",
  "version": "0.1.1",
  "description": "Local Browse Functionality on a Git Repo containing Markdown",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/dox"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "scripts": {
    "build": "cd src && NODE_ENV=production rollup --config config/rollup.config.js",
    "clean": "rm -f -r .cache/* dist/*",
    "start": "cd src && NODE_ENV=development rollup --watch --config config/rollup.config.js",
    "test": "npx jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "__tests__/.*\\.(ts|tsx)$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "loglevel": "^1.8.0",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "underscore": "^1.13.2",
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-serve": "^1.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}
