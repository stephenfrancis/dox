{
  "name": "dox",
  "version": "0.1.1",
  "description": "Local Browse Functionality on a Git Repo containing Markdown",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/ts-node src/config/fuse.ts",
    "clean": "rm -f -r .cache/* dist/*",
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node src/config/fuse.ts",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/dox"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "__tests__/.*\\.(ts|tsx)$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "fuse-box": "^4.0.1-next.7",
    "jest": "^27.0.6",
    "loglevel": "^1.6.8",
    "marked": "^4.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "ultistore": "0.0.3",
    "viz.js": "^2.1.2"
  }
}
