{
  "name": "@stephen.francis/dox",
  "version": "0.2.4",
  "description": "Local Browse Functionality on a Git Repo containing Markdown",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/dox"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "scripts": {
    "build": "NODE_ENV=production esbuild ./src/app/App.tsx --bundle --minify --sourcemap --platform=browser --define:global=window --outdir=./dist",
    "clean": "rm -f -r dist/*",
    "serve": "node ./src/Serve.js",
    "start": "cd src && NODE_ENV=development rollup --watch --config config/rollup.config.js",
    "test": "npx jest"
  },
  "bin": {
    "serve-dox": "./src/Serve.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": [
        "ts-jest",
        {
          "diagnostics": {
            "ignoreCodes": [
              151001
            ]
          }
        }
      ]
    },
    "testRegex": "__tests__/.*\\.(ts|tsx)$"
  },
  "dependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "koa": "^2.13.4",
    "koa-send": "^5.0.1",
    "loglevel": "^1.8.0",
    "marked": "^4.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "underscore": "^1.13.4"
  },
  "devDependencies": {
    "@types/jest": "^29.0.2",
    "esbuild": "^0.15.7",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  }
}
